<h2 class="page-header">Recently Added</h2>
<div class="books-recently-added">
    <% books.forEach(book => { %>
        <div class="books-recently-added-inner">
            <a href="/books/<%= book.id %>"><img src="<%= book.coverImagePath %>" style="height:300px;">   
             <!-- lager 'variable' i databasen som henter path for oss --></a>
             <div class="book-title"><%= book.title %></div>
        </div>
    <% }) %>
</div>
