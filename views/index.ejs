<h2 class="page-header">Recently Added</h2>
<div>
    <% books.forEach(book => { %>
        <a href="/books/<%= book.id %>">
            <img src="<%= book.coverImagePath %>" style="height:150px;">    <!-- lager 'variable' i databasen som henter path for oss -->
        </a>
        <div><%= book.title %></div>
    <% }) %>
</div>